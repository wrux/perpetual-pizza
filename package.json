{
  "name": "perpetual-pizza",
  "version": "1.0.0",
  "description": "The Never Ending Digital Pizza.",
  "main": "src/index.html",
  "scripts": {
    "dev": "parcel src/index.html",
    "build": "parcel build src/index.html",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wrux/perpetual-pizza.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wrux/perpetual-pizza/issues"
  },
  "homepage": "https://github.com/wrux/perpetual-pizza#readme",
  "devDependencies": {
    "lit-element": "^2.4.0",
    "parcel-bundler": "^1.12.5",
    "parcel-plugin-pwa-manifest": "^3.0.10",
    "parcel-plugin-sw-cache": "^0.3.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {},
  "cache": {
    "swDest": "service-worker.js",
    "globPatterns": ["**/*.{html,js,css,webp,png,mp4}"],
    "navigateFallback": "/index.html",
    "templatedURLs": {
      "/": ["index.html"]
    }
  },
  "pwaManifest": {
    "name": "Perpetual Pizza",
    "shortName": "Perpetual Pizza",
    "startURL": "./",
    "theme": "#d00000",
    "generateIconOptions": {
      "appleTouchIconBG": "#ffffff",
      "appleTouchIconPadding": 24,
      "baseIcon": "./src/assets/icon.svg",
      "genFavicons": true,
      "sizes": [
        192,
        384,
        512
      ]
    }
  }
}
